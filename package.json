{
	"name": "worklog",
	"displayName": "worklog",
	"description": "create a worklog in markdown format",
	"version": "0.0.5",
	"publisher": "chaturaloka",
	"repository": {
		"type": "git",
		"url": "https://github.com/chaturaloka/vscode-worklog"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.logWork"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.logWork",
				"title": "Create a Work log"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.logWork",
					"group": "createWorkLog"
				}
			],
			"editor/title/context": [
				{
					"command": "extension.logWork",
					"group": "createWorkLog"
				}
			],
			"explorer/context": [
				{
					"command": "extension.logWork",
					"group": "navigation@2"
				}
			]
		},
		"configuration": {
			"title": "WorkLog",
			"properties": {
				"WorkLog.plan": {
					"type": [
						"array",
						"null"
					],
					"default": [
						"# Plan for the day\n\n",
						"- [ ] Misc\n\n",
						"---\n\n",
						"## Misc\n\n",
						"---\n"
					],
					"description": "Specifies the content of the Plan for the day"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}